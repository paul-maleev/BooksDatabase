<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>


<AnchorPane prefHeight="480.0" prefWidth="640.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.booksdatabase.HelloController">
   <children>
      <TableView fx:id="booksTableView" layoutX="14.0" layoutY="14.0" prefHeight="459.0" prefWidth="481.0">
        <columns>
          <TableColumn editable="false" prefWidth="75.0" text="ISBN" />
          <TableColumn prefWidth="75.0" text="Title" />
            <TableColumn prefWidth="75.0" text="year" />
        </columns>
      </TableView>
      <Button fx:id="saveToFileButton" layoutX="500.0" layoutY="49.0" mnemonicParsing="false" onAction="#onSaveButtonClick" prefHeight="25.0" prefWidth="126.0" text="Save" />
      <Button fx:id="connectToDBButton" layoutX="500.0" layoutY="14.0" mnemonicParsing="false" onAction="#onConnectToDBButtonClick" prefHeight="25.0" prefWidth="126.0" text="Open" />
   </children>
</AnchorPane>
